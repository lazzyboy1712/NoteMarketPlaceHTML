    <!-- Title -->
    <title>Notes Details</title>

    <!-- Favicon -->
    <link rel="shortcut icon" href="../img/favicon/favicon.ico">

    <!-- google fonts --->
    <link href="https://fonts.googleapis.com/css2?family=Open+Sans:wght@300;400;600;700;800&display=swap" rel="stylesheet">

    <!-- Fontawesome -->
    <link rel="stylesheet" href="../css/font-awesome/css/font-awesome.min.css">

    <!-- Bootstrap CSS -->
    <link rel="stylesheet" href="../css/bootstrap/bootstrap.min.css">

    <!-- owl-carousel -->
    <link rel="stylesheet" href="../css/owl-carousel/owl.carousel.min.css">
    <link rel="stylesheet" href="../css/owl-carousel/owl.theme.default.min.css">

    <!-- Custom CSS -->
    <link rel="stylesheet" href="../css/style.css">

    <!-- Responsive CSS -->
    <link rel="stylesheet" href="../css/responsive.css">

</head>

<body>

    <!-- Preloader -->
    <div id="preloader">
        <div id="status"></div>
    </div>

    <!-- Header -->
    <header>
        <nav class="navbar navbar-fixed-top white-nav-top">
            <div class="container-fluid">
                <div class="site-nav-wrapper">
                    <div class="navbar-header">

                        <!-- Mobile Menu Open Button -->
                        <span id="mobile-nav-open-btn">&#9776;</span>

                        <!-- logo -->
                        <a href="#" class="navbar-brand">
                            <img src="../img/home/logo.png" alt="logo" id="logo-img">
                        </a>
                    </div>

                    <!-- Main Menu -->
                    <div class="container" id="main-menu">
                        <div class="collapse navbar-collapse">
                            <ul class="nav navbar-nav pull-right">
                                <li>@Html.ActionLink("Search Notes", "SearchNotes", "pages", null, new { @class = "smooth-scroll" })</li>
                                <li>@Html.ActionLink("Sell Your Notes", "AddNotes", "pages", null, new { @class = "smooth-scroll" })</li>
                                <li>@Html.ActionLink("Buyer Request", "BuyerRequest", "pages", null, new { @class = "smooth-scroll" })</li>
                                <li>@Html.ActionLink("FAQ", "Faq", "pages", null, new { @class = "smooth-scroll" })</li>
                                <li>@Html.ActionLink("Contact Us", "ContactUs", "pages", null, new { @class = "smooth-scroll" })</li>
                                <li><a href="#" class="u-img"><img src="~/Content/img/Customer/cust-1.jpg" alt="user" class="img-circle" id="user-image-add-notes"></a></li>
                                <li>@Html.ActionLink("LOGOUT", "LogIn", "pages", null, new { @class = "smooth-scroll", @id = "navigation-login-btn" })</li>
                            </ul>
                        </div>
                    </div>

                    <!-- Mobile Menu -->
                    <div id="mobile-nav">
                        <span id="mobile-nav-close-btn">&times;</span>

                        <div id="mobile-nav-content">
                            <ul class="nav">
                                <li>@Html.ActionLink("Search Notes", "SearchNotes", "pages", null, new { @class = "smooth-scroll" })</li>
                                <li>@Html.ActionLink("Sell Your Notes", "AddNotes", "pages", null, new { @class = "smooth-scroll" })</li>
                                <li>@Html.ActionLink("FAQ", "Faq", "pages", null, new { @class = "smooth-scroll" })</li>
                                <li>@Html.ActionLink("Contact Us", "ContactUs", "pages", null, new { @class = "smooth-scroll" })</li>
                                <li>@Html.ActionLink("LOGIN", "LogIn", "pages")</li>
                            </ul>
                        </div>
                    </div>

                </div>
            </div>
